{
    "editor.wordWrap": "off",
    "files.exclude": {
			"**/node_modules": {},
			"typings": {},
			"**/*.map": {},
			"**/*.js": {
				"when": "$(basename).ts"
			}
	},
	"appService.deploySubpath": "dist",
	"typescript.tsdk": "node_modules/typescript/lib",
	"compile-hero.disable-compile-files-on-did-save-code": true
}
